{
  "name": "html-webpack-change-assets-extension-plugin",
  "version": "0.0.0-development",
  "description": "Add customized extension to index.html file's script that generated by html-webpack-plugin.",
  "keywords": [
    "brotli",
    "compress",
    "change script extension",
    "html-webpack-plugin",
    "plugin",
    "webpack"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nerdmax/html-webpack-change-assets-extension-plugin.git"
  },
  "license": "MIT",
  "author": "Max <mrliucan@foxmail.com>",
  "files": [
    "dist"
  ],
  "main": "dist/html-webpack-change-assets-extension-plugin.umd.js",
  "module": "dist/html-webpack-change-assets-extension-plugin.es5.js",
  "typings": "dist/types/html-webpack-change-assets-extension-plugin.d.ts",
  "scripts": {
    "prebuild": "rimraf dist docs",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "build:doc": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "check-ci": "yarn format:ci && yarn test:prod && yarn build && yarn build:doc",
    "precommit": "lint-staged",
    "commit": "git-cz",
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS",
    "contributors:add": "all-contributors add",
    "contributors:check": "all-contributors check",
    "contributors:generate": "all-contributors generate",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "format": "yarn prettier --write",
    "format:ci": "yarn prettier --list-different",
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prepublishOnly": "npm run test && npm run build",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest",
    "test:htmlWebpackPlugin3": "yarn add 'html-webpack-plugin@^3.2.0 || ^4.0.0-beta.4' && yarn test --coverageDirectory='coverage-HtmlWebpackPlugin3'",
    "test:htmlWebpackPlugin4": "yarn add 'html-webpack-plugin@^4.0.0-beta.4' && yarn test --coverageDirectory='coverage-HtmlWebpackPlugin4'",
    "test:prod": "yarn type-check && yarn run lint && yarn run test:htmlWebpackPlugin4 && yarn run test:htmlWebpackPlugin3",
    "test:watch": "jest --watch",
    "travis-deploy-once": "travis-deploy-once",
    "prettier": "prettier '**/*.{js,ts,tsx,css,json}'",
    "type-check": "tsc --noEmit"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 95,
        "lines": 80,
        "statements": 80
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.12.9",
    "all-contributors-cli": "^5.4.1",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-jest": "^23.2.0",
    "colors": "^1.3.2",
    "commitizen": "^3.0.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "html-webpack-plugin": "^3.2.0 || ^4.0.0-beta.4",
    "husky": "^1.0.1",
    "jest": "^23.6.0",
    "jest-config": "^23.6.0",
    "lint-staged": "^8.0.0",
    "lodash.camelcase": "^4.3.0",
    "memory-fs": "^0.4.1",
    "prettier": "^1.14.3",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "rollup": "^1.0.0",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.18.0",
    "semantic-release": "^15.10.3",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^23.10.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.14.0",
    "typescript": "^3.0.3",
    "webpack": "^4.21.0"
  },
  "peerDependencies": {
    "html-webpack-plugin": "^3.2.0 || ^4.0.0-beta.4",
    "webpack": "^4.21.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  }
}
